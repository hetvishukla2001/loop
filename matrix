#include<stdio.h>
int main(){
	int max[3][3];
	int i,j,sum=0;
	
	for(i=0;i<3;i++){
		for(j=0;j<3;j++){
			printf("enter 3*3 matrix [%d][%d] element : ",i,j);
			scanf("%d",&max[i][j]);
			if(i==j){
				sum=sum+max[i][j];
				
			}
		}
	}
	
	printf("\n your elements are : ");
	for(i=0;i<3;i++){
		for(j=0;j<3;j++){
			printf(" %d",max[i][j]);
		}
	}
	printf(" \ndigonal element sum is  %d ",sum);
	return 0;
	
}
